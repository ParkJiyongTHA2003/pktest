@{
    ViewData["Title"] = "Forgot Password";
}

<h2>Forgot Password</h2>

<form asp-action="ForgotPassword" method="post">
    <div>
        <label for="Email">Email:</label>
        <input type="email" id="Email" name="email" required />
    </div>
    <button type="submit">Submit</button>
</form>
